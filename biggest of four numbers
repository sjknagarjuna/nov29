a=int(input("enter the value"))
b=int(input("enter the value"))
c=int(input("enter the value"))
d=int(input("enter the value"))
if a>b:
    if a>c:
        if a>d:
            print("a is big")

if b>a:
    if b>c:
        if b>d:
             print("b is big")
if c>a:
     if c>b:
          if a>d:
            print("c is big")

if d>a:
    if d>b:
        if a>c:
            print("d is big")           
else:
    print("all are equal")
